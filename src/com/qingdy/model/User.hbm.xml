<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.qingdy.model.User" table="blog">
		<id name="username" column="username" unsaved-value="null">
		</id>
		<property name="password" column="password" length="50" not-null="true">
		</property>
	</class>
</hibernate-mapping>